{
  "dts":true,
  "compilerOptions": {

    "target": "ES6",                                
    "module": "commonjs",
    "esModuleInterop": true,                           
    "forceConsistentCasingInFileNames": true,            
    "strict": true,                                      
    "skipLibCheck": true,
    "noUncheckedIndexedAccess": true,
    "noEmit": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "paths": {
      "*": ["src/*"],
      "typings/*": ["typings/*"]
    }
  }
}
